{
  "scripts": {},
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,yml,json}": "prettier --write",
    "package.json": "prettier-package-json --write"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}

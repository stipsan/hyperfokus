{
  "extends": ["github>finn-no/renovate-presets:js"],
  "masterIssue": true,
  "lockFileMaintenance": {
    "automerge": true
  },
  "packageRules": [
    {
      "depTypeList": ["resolutions"],
      "automerge": true
    },
    {
      "packageNames": ["next"],
      "followTag": "canary"
    },
    {
      "packageNames": ["react", "react-dom"],
      "followTag": "experimental"
    },
    {
      "packageNames": ["webpack"],
      "followTag": "next"
    }
  ]
}
